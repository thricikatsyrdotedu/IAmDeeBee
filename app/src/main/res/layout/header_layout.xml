<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/header_layout_image_view_activity_1"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_centerVertical="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/navigation_view_header_logo_activity1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/header_layout_text_view_1_activity_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="44dp"
        android:text="header_layout_text_view1"
        android:textColor="#FFFFFF"
        android:textSize="10pt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header_layout_image_view_activity_1" />
    <TextView
        android:id="@+id/header_layout_text_view2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="header_layout_text_view_1_activity_1"
        android:textColor="#FFFFFF"
        android:textSize="6pt"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/header_layout_text_view_1_activity_1" />
</androidx.constraintlayout.widget.ConstraintLayout>